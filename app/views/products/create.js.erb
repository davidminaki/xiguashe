


$("#id_link").val();
$("#product_form").modal('hide');

$("#product-ids").append('<p id="p_<%= @product.id %>"><input id="<%= @product.id %>" type="hidden" value="<%= @product.id %>" name="product_id[]" >');
$("#<%= @product.id %>").after('<%= link_to @product.title, @product.url, :id => "link_#{@product.id}" %>');
$("#link_<%= @product.id %>").after('<%= link_to "X", @product, method: :delete, confirm: "是否确定删除", remote: true %></p>');
$("#module_win").html('')
